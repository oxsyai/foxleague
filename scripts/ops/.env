# Network + wallet
MX_NETWORK=devnet
MX_WALLET=/absolute/path/to/owner.pem

# Deploy
SUPER_USER=erd1...your_super_user...

# Init (owner-only)
FEES_RECEIVER=erd1...fees...
PRIZES_RECEIVER=erd1...prizes...
TOKEN_ID=FOXSY-xxxxxx
MIN_REGISTRATION_FEE=1000000000000000000     # 1 token (18 dec)
MIN_PRIZE=100000000000000000000              # 100 tokens
MIN_REG_TIME=86400                           # 24h
# optional:
# CREATOR_WHITELIST=erd1...creator...

# Create tournament
T_ID=1
# START/END optional; auto-defaults if not set
# START=...
# END=...
REG_FEE=10000000000000000000                 # 10 tokens
PRIZE=1000000000000000000000                 # 1000 tokens

# Register
TOURNAMENT_ID=1
TEAM_SETUP_ID=42
FEE_AMOUNT=10000000000000000000              # 10 tokens
